<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter>
  <LastVersion value="-1" relOp="NEQ"/>
  <And>
    <Class name="de.fzj.unicore.xuudb.client.AdminController"/>
    <Bug pattern="OS_OPEN_STREAM"/>
    <Or>
      <Method name="import_csv"/>
      <Method name="export_csv"/>
    </Or>
  </And>
  <And>
    <Class name="de.fzj.unicore.xuudb.client.AdminController"/>
    <Bug pattern="DE_MIGHT_IGNORE"/>
    <Method name="checkBatch"/>
  </And>
  <And>
    <Class name="de.fzj.unicore.xuudb.client.AdminController"/>
    <Bug pattern="NP_BOOLEAN_RETURN_NULL"/>
    <Method name="isDNMode"/>
  </And>
</FindBugsFilter>
